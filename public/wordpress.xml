<!--    existing user on the blog or to create a new user -->
<!-- 6. WordPress will then import each of the posts, comments, and categories -->
<!--    contained in this file into your blog -->

<!-- generator="WordPress/MU" created="2008-05-16 22:49"-->
<rss version="2.0"
	xmlns:content="http://purl.org/rss/1.0/modules/content/"
	xmlns:wfw="http://wellformedweb.org/CommentAPI/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:wp="http://wordpress.org/export/1.0/"
	>

	<channel>
		<title>{=title.text}</title>
		<link>http</link>
		<description>{=sub_title.text}</description>
		<!--<pubDate></pubDate>-->
		<generator>http://wordpress.org/?v=MU</generator>
		<language>en</language>
		<wp:wxr_version>1.0</wp:wxr_version>
		<wp:base_site_url>{=attr.root_url}</wp:base_site_url>
		<wp:base_blog_url>{=attr.root_url}</wp:base_blog_url>
		{:reduce categories.category}
		<wp:category>
			<wp:category_nicename>{=slug title.text}</wp:category_nicename><wp:category_parent></wp:category_parent>
			<wp:cat_name><![CDATA[{=title.text}]]></wp:cat_name>
		</wp:category>
		{/:reduce}
		{:r tags}
		<wp:tag><wp:tag_slug>{=slug tag}</wp:tag_slug><wp:tag_name><![CDATA[{=slug tag}]]></wp:tag_name></wp:tag>
		{/:r}
		{:reduce posts.post}
		<item>
			<title>{=title.text}</title>
			<link>{=attr.post_url}</link>
			<pubDate>{=attr.date_created}</pubDate>
			<dc:creator>{:r authors.author}{=attr.ref}{/:r}</dc:creator>
			{:s categories}{:r category}
			<category><![CDATA[{=attr.ref}]]></category>
			<category domain="category" nicename="{=slug attr.ref}"><![CDATA[{=attr.ref}]]></category>
			{/:r}{/:s}
			{:s tags}{:r tag}
			<category domain="tag" nicename="{=slug attr.ref}"><![CDATA[{=attr.ref}]]></category>
			{/:r}{/:s}
			<guid isPermaLink="true">{=attr.id}</guid>
			<description></description>
			<content:encoded><![CDATA[{=content.text}]]></content:encoded>
			<wp:post_id>{=attr.id}</wp:post_id>
			<wp:post_date>{=attr.date_created}</wp:post_date>
			<wp:post_date_gmt>{=attr.date_created}</wp:post_date_gmt>
			<wp:comment_status>open</wp:comment_status>
			<wp:ping_status>open</wp:ping_status>
			<wp:post_name>{=uri slug attr.post_url}</wp:post_name>
			<wp:status>publish</wp:status>
		  <!--<wp:post_parent>0</wp:post_parent>-->
			<!--<wp:menu_order>0</wp:menu_order>-->
			<wp:post_type>post</wp:post_type>
			<!--<wp:post_password></wp:post_password>-->
			<!--<wp:postmeta>-->
				<!--<wp:meta_key>_edit_lock</wp:meta_key>-->
				<!--<wp:meta_value>1210966825</wp:meta_value>-->
			<!--</wp:postmeta>-->
			<!--<wp:postmeta>-->
				<!--<wp:meta_key>_edit_last</wp:meta_key>-->
				<!--<wp:meta_value>2575641</wp:meta_value>-->
			<!--</wp:postmeta>-->
			{:s comments}{:reduce comment}
			<wp:comment>
				<wp:comment_id>{=attr.id}</wp:comment_id>
				<wp:comment_author><![CDATA[{=attr.user_name}]]></wp:comment_author>
				<wp:comment_author_email>{=attr.user_email}</wp:comment_author_email>
				<wp:comment_author_url>{=attr.user_url}</wp:comment_author_url>
				<wp:comment_author_IP>{=attr.user_ip}</wp:comment_author_IP>
				<wp:comment_date>{=attr.date_created}</wp:comment_date>
				<wp:comment_date_gmt>{=attr.date_created}</wp:comment_date_gmt>
				<wp:comment_content>{=content.text}</wp:comment_content>
				<wp:comment_approved>1</wp:comment_approved>
				<wp:comment_type></wp:comment_type>
				<wp:comment_parent>{=attr.parentid}</wp:comment_parent>
				<!--<wp:comment_user_id>2575641</wp:comment_user_id>-->
			</wp:comment>
			{/:reduce}{/:s}
		</item>
		{/:reduce}
	</channel>
</rss>


